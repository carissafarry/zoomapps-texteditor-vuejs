{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2022",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./server/@types",
      "./node_modules/@types"
    ]
  },
}
