{
  "name": "za-server",
  "version": "0.1.0",
  "description": "A template express server for Zoom Apps",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "preprepare": "[ -d .git ] && [ -f .env ] && git update-index --assume-unchanged .env",
    "prepare": "cd .. && npx husky install server/.husky"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "~4.17.3",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "https-localhost": "^4.7.1",
    "mongoose": "^6.2.3",
    "mongoose-encryption": "^2.1.2",
    "morgan": "~1.10.0",
    "pug": "^3.0.2"
  },
  "peerDependencies": {
    "mongodb": "^4.4.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.4.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "private": true,
  "license": "MIT"
}
