{
  "name": "hello-zoom-apps",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/server",
  "types": "dist/server",
  "private": true,
  "scripts": {
    "start": "docker compose up zoom-app",
    "build": "npm run server",
    "prepare": "npx husky install",
    "clean": "docker compose down -v --remove-orphans --rmi local"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
