{
  "name": "hello-zoom-apps",
  "version": "0.1.0",
  "description": "A Zoom App Template",
  "workspaces": [
    "server",
    "app"
  ],
  "scripts": {
    "prestart": "npm run build --workspaces",
    "start": "docker compose up",
    "build": "docker build --build-arg NODE_ENV=production -t zoom-app .",
    "clean": "docker compose down -v --remove-orphans"
  },
  "devDependencies": {
    "husky": "^7.0.4"
  },
  "private": true,
  "license": "MIT"
}
