{
  "name": "collaborative-za-sample",
  "version": "1.0.0",
  "description": "A Zoom App Template",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "workspaces": [
    "server",
    "app"
  ],
  "scripts": {
    "start": "npm --prefix dist run start",
    "build": "scripts/build.sh",
    "dev": "node scripts/dev.cjs",
    "prepare": "husky install",
    "prestart": "[[ -d dist/node_modules ]] || npm run build",
    "postinstall": "node scripts/gen-secrets.mjs"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "dotenv-cli": "^5.1.0",
    "envsub": "^4.0.7",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
