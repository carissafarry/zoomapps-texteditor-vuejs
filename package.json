{
  "name": "hello-zoom-apps",
  "version": "0.1.0",
  "description": "A Zoom App Template",
  "workspaces": [
    "server",
    "app"
  ],
  "scripts": {
    "start": "npm build -w app && npm run start-server",
    "start-app": "npm run start -w app",
    "start-server": "npm run start -w server",
    "build": "npm run build -ws",
    "clean": "npm run clean -w server",
    "prepare": "npx husky install server/.husky"
  },
  "devDependencies": {
    "husky": "^7.0.4"
  },
  "private": true,
  "license": "MIT"
}
